{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug pydantify CLI",
            "type": "debugpy",
            "request": "launch",
            "module": "pydantify",
            "args": [
                "./temp/srl_nokia-dot1x.yang",
                "./temp/srl_nokia-platform-healthz.yang",
                "./temp/srl_nokia-interfaces.yang",
                "-p",
                "${workspaceFolder}/temp/iana",
                "-p",
                "${workspaceFolder}/temp/ietf",
                "-p",
                "${workspaceFolder}/temp/models",
                "-o",
                "./models/interfaces"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYDANTIFY_TMP_DIR": "./temp",
                "PYDANTIFY_IANA_PATH": "${env:PYDANTIFY_IANA_PATH}",
                "PYDANTIFY_IETF_PATH": "${env:PYDANTIFY_IETF_PATH}",
                "PYDANTIFY_SRL_MODELS_PATH": "${env:PYDANTIFY_SRL_MODELS_PATH}",
                "MODEL_SHORT_NAME": "interfaces"
            },
            "justMyCode": false
        }
    ]
}